<Application x:Class="ProxyForwarder.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProxyForwarder.App.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
            <ResourceDictionary Source="pack://application:,,,/Assets/styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <local:RemainingTimeConverter x:Key="RemainingTimeConverter"/>
            <local:StatusToButtonTextConverter x:Key="StatusToButtonTextConverter"/>
            <local:LocalPortConverter x:Key="LocalPortConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
